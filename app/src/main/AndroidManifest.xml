<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.intigritirobotics">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:requestLegacyExternalStorage="true"
        android:allowBackup="true"
        android:icon="@drawable/test_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/test_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.IntigritiRobotics">
        <activity android:name=".e_store.ProjectPdfActivity"
            android:theme="@style/Theme.IntigritiRobotics.NoActionBar"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity android:name=".e_store.MyOrderDetailActivity"
            android:theme="@style/Theme.IntigritiRobotics.NoActionBar"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity android:name=".e_store.CheckOutActivity"
            android:theme="@style/Theme.IntigritiRobotics.NoActionBar"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity android:name=".e_store.UpdateUserDetails"
            android:theme="@style/Theme.IntigritiRobotics.NoActionBar"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".e_store.ProductDetailActivity"
            android:parentActivityName=".e_store.MainHomeActivity"
            android:theme="@style/Theme.IntigritiRobotics.NoActionBar" />
        <activity
            android:name=".e_store.ui.Support.SupportActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".e_store.ui.Setting.SettingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".e_store.ui.MyCart.MyCartActivity"
            android:theme="@style/Theme.IntigritiRobotics.NoActionBar"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".e_store.ui.MyAccount.UserProfileActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".e_store.ViewAllActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".e_store.MainHomeActivity"
            android:label="@string/title_activity_main_home"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity"
            android:theme="@style/Theme.IntigritiRobotics.NoActionBar" />
        <activity
            android:name=".e_store.SplashActivity"
            android:theme="@style/Theme.IntigritiRobotics.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".e_store.ResetActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".e_store.SignInActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".e_store.SignUpActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"
            tools:ignore="LockedOrientationActivity" />

        <meta-data
            android:name="com.razorpay.ApiKey"
            android:value="rzp_test_6fL9gcabPXqYqj" />
    </application>

</manifest>